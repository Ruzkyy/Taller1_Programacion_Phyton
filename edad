from time import localtime
t = localtime()
dia = t.tm_mday
mes = t.tm_mon
anno = t.tm_year
print("ingrese su fecha de nacimiento")
dianaci = int(input("Dia:"))
mesnaci = int(input("mes:"))
annonaci = int(input("año:"))
if (mesnaci, dianaci) < (mes, dia):
    edad = anno - annonaci
else:
    edad = anno - annonaci - 1
print("usted tiene "+ str(edad) + " años")


